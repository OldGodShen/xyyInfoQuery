{
  "productName": "xyyinfoquery",
  "version": "0.0.5",
  "identifier": "com.oldgodshen.xyyinfoquery",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1520",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "新迎园信息查询",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "android": {
      "minSdkVersion": 24
    },
    "active": true,
    "targets": [
      "nsis",
      "msi",
      "updater"
    ],
    "publisher": "OldGodShen",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "allowDowngrades": false,
      "certificateThumbprint": null,
      "digestAlgorithm": null,
      "signCommand": null,
      "timestampUrl": null,
      "tsp": false,
      "webviewFixedRuntimePath": null,
      "webviewInstallMode": {
        "silent": false,
        "type": "downloadBootstrapper"
      },
      "wix": {
        "language": "zh-CN"
      },
      "nsis": {
        "languages": [
          "SimpChinese",
          "English"
        ]
      }
    }
  }
}